{
    "name": "Guelph Drop-In Sync",
    "version": "1.0.1",
    "description": "UoG Fitness Centre x Google Calendar",
    "author": "Eric Buys",
    "manifest_version": 3,
    "key": "",
    "oauth2": {
        "client_id": "504537351045-rntjk2p9a9lejjq0t0fnuntcdr0e7k8m.apps.googleusercontent.com",
        "scopes": [
            "profile email",
            "https://www.googleapis.com/auth/calendar"
        ]
    },
    "action":{
        "default_popup": "./html/index.html"
    },
    "permissions": [
        "storage",
        "identity",
        "offscreen",
        "alarms"
    ],
    "host_permissions": [
        "https://fitandrec.gryphons.ca/*"
    ],
    "background": {
        "service_worker": "./js/background.js",
        "type": "module"
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}