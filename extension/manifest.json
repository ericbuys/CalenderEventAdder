{
    "name": "DropInSync",
    "version": "1.0",
    "description": "Syncs the Fit & Rec Drop-In Calendar by Activity with your Google Calendar",
    "author": "Eric Buys",
    "action":{
        "default_popup": "./html/index.html"
    },
    "permissions": [
        "storage",
        "nativeMessaging",
        "http://127.0.0.1:8000/*"
    ],
    "background": {
        "service_worker": "./js/background.js"
    },
    "manifest_version": 3
}