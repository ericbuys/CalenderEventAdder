{
    "name": "DropInSync",
    "version": "1.0",
    "description": "Syncs the Fit & Rec Drop-In Calendar by Activity with your Google Calendar",
    "author": "Eric Buys",
    "manifest_version": 3,
    "key": "",
    "oauth2": {
        "client_id": "504537351045-rntjk2p9a9lejjq0t0fnuntcdr0e7k8m.apps.googleusercontent.com",
        "scopes": [
            "profile email",
            "https://www.googleapis.com/auth/calendar"
        ]
    },
    "action":{
        "default_popup": "./html/index.html"
    },
    "permissions": [
        "storage",
        "nativeMessaging",
        "identity",
        "identity.email"
    ],
    "host_permissions": [
        "https://fitandrec.gryphons.ca/*"
    ],
    "background": {
        "service_worker": "./js/background.js"
    }
}